<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
 <default>
		<ced_csmultishipping>
			<shipping>
				<methods>
					<perkmshipping>
						<model>Ced\CsPerkmshipping\Model\Vsettings\Shipping\Methods\Perkmshipping</model>
						<config_path>ced_csperkmshipping/general/active</config_path>
					</perkmshipping>
				</methods>
			</shipping>
		</ced_csmultishipping>
        <carriers>
            <perkmshipping>
                <active>1</active>
                 <title>Vendor Distance Based Shipping</title>
                <name>Vendor Distance Based Shipping</name>
                <specificerrmsg>This Shipping is currently unavailable</specificerrmsg>
                <model>Ced\CsPerkmshipping\Model\Carrier\Perkmshipping</model>
            </perkmshipping>
        </carriers>
        <ced_csperkmshipping>
        	<general>
        		<active>1</active>
        	</general>
        </ced_csperkmshipping>
    </default>
</config>
