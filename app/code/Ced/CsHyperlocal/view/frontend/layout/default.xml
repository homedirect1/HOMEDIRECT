<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Ced_CsHyperlocal::css/hyperlocal_default.css"/>
        <css src="Ced_CsHyperlocal::css/font-awesome.min.css"/>
    </head>
    <referenceBlock name="head.additional">
        <block class="Magento\Framework\View\Element\Template" name="ced_hyperlocal_google_api" template="Ced_CsHyperlocal::head_api.phtml" />
    </referenceBlock>
    <body>
        <referenceBlock name="header.links">
            <block class="Magento\Framework\View\Element\Template" name="ced.cshyperlocal.header.location"
                   template="Ced_CsHyperlocal::header.phtml" cacheable="false"/>
        </referenceBlock>
        <referenceContainer name="header-wrapper">
            <block class="Magento\Framework\View\Element\Template" name="ced.cshyperlocal.mobile.header.location"
                   template="Ced_CsHyperlocal::mobile_header.phtml" cacheable="false"/>
        </referenceContainer>
       
    </body>
</page>
