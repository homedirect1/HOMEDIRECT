<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Ced_CsHyperlocal::cshyperlocal" title="Hyperlocal System" sortOrder="40">
                    <resource id="Ced_CsHyperlocal::shipping_area" title="Shipping Area" sortOrder="10">
                        <resource id="Ced_CsHyperlocal::shippingarea" title="Shipping Area" sortOrder="10" />
                    </resource>
                    <resource id="Ced_CsHyperlocal::hyperlocal_configuration" title="Configuration" sortOrder="10">
                        <resource id="Ced_CsHyperlocal::configuration" title="Configuration" sortOrder="10" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Ced_CsHyperlocal::ced_cshyperlocal" title="Hyperlocal System" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>