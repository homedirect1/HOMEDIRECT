<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="vendorpanel"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="ced_hyperlocal_google_api" remove="true" />
        <referenceBlock name="csmarketplace_vendor_navigation">
            <block class="Ced\CsMarketplace\Block\Link\Current" name="ced_hyperlocal_1111123"
                   ifconfig="ced_cshyperlocal/general/activation">
                <arguments>
                    <argument name="name" xsi:type="string">ced_hyperlocal_1111123</argument>
                    <argument name="label" xsi:type="string" translate="true">Delivery Locations</argument>
                    <argument name="path" xsi:type="string">cshyperlocal/shiparea/index</argument>
                    <argument name="font_awesome" xsi:type="string">fa fa-map-marker</argument>
                    <argument name="sort_order" xsi:type="string">50</argument>
                    <argument name="level" xsi:type="string">5</argument>
                </arguments>
            </block>
        </referenceBlock>

    </body>
</page>
